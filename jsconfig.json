{
  "$schema": "https://www.schemastore.org/jsconfig.json",
  "compilerOptions": {
    "lib": ["ES5", "ES2015"],
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "checkJs": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
